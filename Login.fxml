<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="391.0" prefWidth="577.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="puppycentral.LoginController">
   <children>
      <ImageView fitHeight="391.0" fitWidth="644.0" layoutX="-3.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/LOGIN.jpeg" />
         </image>
      </ImageView>
      <TextField fx:id="tuser" layoutX="94.0" layoutY="87.0" prefHeight="26.0" prefWidth="187.0" />
      <PasswordField fx:id="tpassword" layoutX="94.0" layoutY="126.0" prefHeight="26.0" prefWidth="187.0" />
      <Button fx:id="forgotpass" layoutX="94.0" layoutY="160.0" mnemonicParsing="false" onAction="#forgotAction" prefHeight="35.0" prefWidth="187.0" style="-fx-background-color: #ffffff00;" text="Forgot Password?" textFill="#00cbff">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <ImageView fitHeight="26.0" fitWidth="23.0" layoutX="55.0" layoutY="89.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/USERNAME.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="23.0" fitWidth="16.0" layoutX="58.0" layoutY="131.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/PASSWORD%201.png" />
         </image>
      </ImageView>
      <Button fx:id="login" layoutX="144.0" layoutY="196.0" mnemonicParsing="false" onAction="#loginAction" prefHeight="26.0" prefWidth="73.0" text="Login" />
      <Button fx:id="back" layoutX="495.0" layoutY="344.0" mnemonicParsing="false" onAction="#backAction" text="Back" />
      <Button fx:id="signup" layoutX="435.0" layoutY="23.0" mnemonicParsing="false" onAction="#signUpAction" style="-fx-background-color: #0000FF;" text="New User? SignUp" textFill="#0bdbd6">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
