<AnchorPane prefHeight="416.0" prefWidth="723.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="puppycentral.SignUpController">
   <children>
      <ImageView fitHeight="416.0" fitWidth="307.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/signup1.jpg" />
         </image>
      </ImageView>
      <Button fx:id="login" layoutX="324.0" layoutY="21.0" mnemonicParsing="false" onAction="#loginAction" text="Login" />
      <Button fx:id="tnc" layoutX="524.0" layoutY="21.0" mnemonicParsing="false" onAction="#tncAction" prefHeight="32.0" prefWidth="178.0" text="Terms and Conditions" />
      <TextField fx:id="tname" layoutX="412.0" layoutY="103.0" prefHeight="31.0" prefWidth="258.0" promptText="USERNAME" />
      <TextField fx:id="temail" layoutX="414.0" layoutY="149.0" prefHeight="31.0" prefWidth="258.0" promptText="EMAIL ID" />
      <TextField fx:id="tcont" layoutX="524.0" layoutY="193.0" prefHeight="31.0" prefWidth="149.0" promptText="CONTACT NO." />
      <TextField fx:id="tpass" layoutX="415.0" layoutY="243.0" prefHeight="31.0" prefWidth="258.0" promptText="PASSWORD" />
      <ImageView fitHeight="26.0" fitWidth="26.0" layoutX="365.0" layoutY="103.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/USERNAME.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="26.0" fitWidth="38.0" layoutX="365.0" layoutY="149.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/EMAIL.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="19.0" fitWidth="20.0" layoutX="369.0" layoutY="198.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/CONTACT.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="19.0" fitWidth="66.0" layoutX="371.0" layoutY="249.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/PASSWORD%201.png" />
         </image>
      </ImageView>
      <CheckBox fx:id="catnc" layoutX="397.0" layoutY="341.0" mnemonicParsing="false" text="Accept Tems and Conditions" />
      <Button fx:id="signup" layoutX="625.0" layoutY="362.0" mnemonicParsing="false" onAction="#SignUpAction" text="SignUp" />
      <ComboBox fx:id="ccode" layoutX="414.0" layoutY="193.0" prefHeight="31.0" prefWidth="85.0" promptText="+91" />
      <Label fx:id="lcaptcha" layoutX="378.0" layoutY="284.0" prefHeight="31.0" prefWidth="96.0" text="Captcha" textFill="#65157d">
         <font>
            <Font name="System Italic" size="21.0" />
         </font></Label>
      <TextField fx:id="tcaptcha" layoutX="485.0" layoutY="287.0" promptText="CAPTCHA" />
   </children>
</AnchorPane>
